{
	"name": "bluezed/yii2-fancytree-widget",
	"description": "The yii2-fancytree widget is a Yii 2 wrapper for Fancytree, a JavaScript dynamic tree view plugin for jQuery with support for persistence, keyboard, checkboxes, tables, drag'n'drop, and lazy loading",
	"keywords": ["yii2", "extension", "widget", "fancytree", "treeview"],
	"type": "yii2-extension",
	"license": "BSD-3-Clause",
	"support": {
		"issues": "https://github.com/bluezed/yii2-fancytree-widget/issues",
		"wiki": "https://github.com/bluezed/yii2-fancytree-widget/wiki/",
		"source": "https://github.com/bluezed/yii2-fancytree-widget"
	},
	"authors": [
		{
			"name": "Thomas Geppert",
			"email": "bluezed.apps@gmail.com"
		}
	],
	"require": {
		"yiisoft/yii2": "*",
		"yiisoft/yii2-jui": "*",
		"bower-asset/fancytree": "*"
	},
	"autoload": {
		"psr-4": { "bluezed\\fancytree\\": "" }
	}
}
